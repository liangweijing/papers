.UCenter-bg {
	background-image: url(https://ftp.bmp.ovh/imgs/2020/05/767e556ae5ea802b.jpg);
	background-size: 100%;
	background-size: cover;
	height: 550rpx;
	display: flex;
	width: 750rpx;
	justify-content: center;
	padding-top: 40rpx;
	overflow: hidden;
	position: relative;
	flex-direction: column;
	align-items: center;
	color: #fff;
	font-weight: 300;
	text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
	border-radius: 0 0 24rpx 24rpx;
}

.UCenter-bg text {
	opacity: 0.8;
}

.UCenter-bg image {
	width: 200rpx;
	height: 200rpx;
}

.UCenter-bg .gif-wave {
	position: absolute;
	width: 100%;
	bottom: 0;
	left: 0;
	z-index: 99;
	mix-blend-mode: screen;
	height: 100rpx;
}

map,
.mapBox {
	left: 0;
	z-index: 99;
	mix-blend-mode: screen;
	height: 100rpx;
}

map,
.mapBox {
	width: 750rpx;
	height: 300rpx;
}
.feedback {
	border-radius: 0;
	background: none;
}
.feedback .cu-btn:not([class*='bg-']) {
	background-color: white;
}
.feedback .cu-btn {
	justify-content: start;
	padding: 0;
}
